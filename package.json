{
  "name": "ai-functions",
  "version": "0.2.19",
  "description": "Library for Developing and Managing AI Functions (including OpenAI GPT4 / GPT3.5)",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "builds": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "format": "prettier --write .",
    "test": "dotenv -- vitest --globals"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanclevenger/ai-functions.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanclevenger/ai-functions/issues"
  },
  "homepage": "https://github.com/nathanclevenger/ai-functions#readme",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 120
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.14",
    "dotenv-cli": "^7.3.0",
    "prettier": "^3.0.3",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "json-schema-to-ts": "^3.0.0",
    "kafkajs": "^2.2.4",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.2.0",
    "openai": "^4.53.2",
    "p-queue": "^7.4.1",
    "partial-json-parser": "^1.2.2",
    "xstate": "^5.0.0-beta.40"
  }
}
